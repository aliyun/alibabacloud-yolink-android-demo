<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.yolink.demo">

    <application
            android:name="com.yolink.demo.DemoApp"
            android:allowBackup="false"
            android:hardwareAccelerated="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.yolink.demo.RewardedVideoAdActivity"/>
        <activity android:name="com.yolink.demo.InterstitialAdActivity" />
        <activity android:name="com.yolink.demo.BannerAdActivity" />
        <activity android:name="com.yolink.demo.NativeAdActivity" />
        <activity android:name="com.yolink.demo.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="ca-app-pub-5980840694381993~3221229369" />
    </application>

</manifest>